<component>
    <name>FrigateHull</name>
    <component_id>2</component_id>
    <category_id>1</category_id>
    <description>The frigate hull, fitted out with everything a frigate needs</description>
    <tpcl_requirements><![CDATA[(lambda (design) 
	(if (= (designType._num-components design) 1) 
		(cons #t \"\") 
		(cons #f \"This is a complete component, nothing else can be included\")))]]></tpcl_requirements>

    <!--propertylist:-->
    <property>
        <name>Armour</name>
        <tpcl_cost><![CDATA[(lambda (design) 4)]]></tpcl_cost>
    </property>
    <property>
        <name>BuildTime</name>
        <tpcl_cost><![CDATA[(lambda (design) 2)]]></tpcl_cost>
    </property>
    <property>
        <name>WeaponWin</name>
        <tpcl_cost><![CDATA[(lambda (design) 2)]]></tpcl_cost>
    </property>
    <property>
        <name>WeaponDraw</name>
        <tpcl_cost><![CDATA[(lambda (design) 0)]]></tpcl_cost>
    </property>
    <property>
        <name>Colonise</name>
        <tpcl_cost><![CDATA[(lambda (design) 1)]]></tpcl_cost>
    </property>
    <property>
        <name>Speed</name>
        <tpcl_cost><![CDATA[(lambda (design) 200000000)]]></tpcl_cost>
    </property>
    <property>
        <name>_num-components</name>
        <tpcl_cost><![CDATA[(lambda (design) 1)]]></tpcl_cost>
    </property>
</component>
