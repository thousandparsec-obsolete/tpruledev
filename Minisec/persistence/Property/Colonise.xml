<property>
    <name>Colonise</name>
    <category>Ships</category>
    <rank>0</rank>
    <display_text>Can Colonise Planets</display_text>
    <description>Can the ship colonise planets</description>
    <tpcl_display><![CDATA[(lambda (design bits)
	(let ((n (apply + bits)))
		(cons n (if (= n 1) \"Yes\" \"No\")) ) )]]></tpcl_display>
