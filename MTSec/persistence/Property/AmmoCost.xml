<property
   description="The relative expensiveness of the explosive"
   display_text="Explosive Unit Cost"
   name="AmmoCost"
   rank="0"
   version="1.1">
   <tpcl_display>(lambda (design bits)
   (let ((n (apply + bits)))
      (cons n (string-append (number-&gt;string n) " credits"))))   </tpcl_display>
