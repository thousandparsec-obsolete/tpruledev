<property
   description="The defensive strength of a ship"
   display_text="Armour"
   name="Armour"
   rank="0"
   version="1.1">
   <tpcl_display>(lambda (design bits)
	(let ((n (apply + bits)))
		(cons n (number-&gt;string n))))   </tpcl_display>
   <tpcl_requires>(lambda (design) (cons #t ""))   </tpcl_requires>
   <!-- categories -->
   <category
      name="Ships" />
</property>
