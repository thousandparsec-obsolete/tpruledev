<property
   category="Ships"
   description="The ship colonise planets"
   display_text="Can Colonise"
   name="Colonise"
   rank="0"
   version="1.0">
   <tpcl_display>(lambda (design bits)
	(let ((n (apply + bits)))
		(cons n (if (= n 1) \"Yes\" \"No\")) ) )   </tpcl_display>
   <tpcl_requires>(lambda (design) (cons #t ""))   </tpcl_requires>
</property>
